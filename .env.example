# Smart Agriculture Dashboard Environment Variables

# API Configuration
VITE_API_URL=http://localhost:3001/api
VITE_IOT_ENDPOINT=ws://localhost:3001/iot

# Weather API (Optional)
VITE_WEATHER_API_KEY=your_weather_api_key_here
VITE_WEATHER_API_URL=https://api.openweathermap.org/data/2.5

# AI/ML Services (Optional)
VITE_CROP_DETECTION_API=https://your-ai-service.com/api
VITE_ML_MODEL_ENDPOINT=https://your-ml-service.com/predict

# Database (Optional)
VITE_DATABASE_URL=your_database_connection_string

# Authentication (Optional)
VITE_AUTH_DOMAIN=your-auth-domain.com
VITE_AUTH_CLIENT_ID=your_auth_client_id

# Analytics (Optional)
VITE_ANALYTICS_ID=your_analytics_id
VITE_SENTRY_DSN=your_sentry_dsn

# Development
VITE_DEV_MODE=true
VITE_LOG_LEVEL=debug